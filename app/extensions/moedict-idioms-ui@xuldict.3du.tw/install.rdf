<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>moedict-idioms-ui@xuldict.3du.tw</em:id>
        <em:name>Moe Idioms Dictionary SearchUI</em:name>
        <em:version>0.0.1</em:version>
        <em:description>Moe Idioms Dictionary SearchUI</em:description>
        <em:creator>Rack Lin(racklin@gmail.com)</em:creator>
        <em:homepageURL>https://github.com/racklin/</em:homepageURL>
        <em:type>2</em:type>
        <em:unpack>true</em:unpack>

    <!-- Gecko Toolkit
         version 1.9.0 and higher -->
    <em:targetApplication>
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>1.9a1</em:minVersion>
        <em:maxVersion>18.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:localized>
      <Description>
        <em:locale>zh</em:locale>
        <em:name>教育部成語典查詢界面</em:name>
        <em:description>教育部成語典查詢界面</em:description>
      </Description>
    </em:localized>

    </Description>
</RDF>

